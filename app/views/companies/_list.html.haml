.element-container
  .title
    %span Newcomers
    %span.small= link_to 'all', companies_path
  .content
    %table.table
      %thead
        %tr
          %th Name
          %th First Trade Date
          %th 1 week growth
          %th 1 month growth
          %th 1 year growth
          %th{:colspan => "3"}
      %tbody
        = render :partial => 'companies/company', :collection => companies